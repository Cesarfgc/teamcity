FROM microsoft/aspnet:4.7
SHELL ["powershell"]
ADD ../../../Binaries/ Binaries/DAL/CoreEndpoint
ADD ../../../SetVariable/ Binaries/SetVariable
ADD ../../../Service.config.TEMPLATE Binaries
COPY procexp64.exe Binaries
ENTRYPOINT C:\Binaries\SetVariable\SetVariable.ps1;\
           &C:\Binaries\procexp64.exe -accepteula;\
           &C:\ServiceMonitor.exe w3svc;\
      
EXPOSE 1001